    <jacidl generateir="true"
      srcdir="idl"
      omgprefix="true"
      includepath="${idl}/omg"
      includes="**/omg/CosNotifyComm.idl,**/omg/CosNotifyFilter.idl"
      destdir="${src}/generated"/>
    <jacidl
      srcdir="${idl}/jacorb"
      includepath="${idl}/omg"
