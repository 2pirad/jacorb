   <target name="comm">
      <jacidl generateir="true"
              srcdir="idl"
              omgprefix="true"
              includepath="${idl}/omg"
              includes="omg/CosEventComm.idl"
              destdir="${src}/generated"/>
   </target>

   <!--                            -->
   <!-- data distribution service  -->
   <!--                            -->

       <target name="dds" depends="local-init">
          <jacidl generateir="true"
                  srcdir="idl"
                  omgprefix="true"
                  includepath="${idl}/omg"
                  includes="omg/dds_dcps.idl"
