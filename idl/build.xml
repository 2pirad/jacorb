   <!-- ==================================================== -->
   <!--                       CORBA                          -->
   <!-- ==================================================== -->

   <target name="jacorb" depends="imr,proxy,sasPolicy,sslPolicy"/>


   <target name="imr">
      <jacidl srcdir="${idl}/jacorb"
                destdir="${src}/generated"
