   <target name="notification"
     depends="local-init,notif,typed,jms-interop"
     description="Compile Notification Service IDL">
		<idl-javac
			javac-includes="org/omg/CosNotification/*.java,org/omg/CosNotify**/*.java,org/omg/CosTyped*/**/*.java"/>
   </target>

