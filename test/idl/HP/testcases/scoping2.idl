 *         compiler
 */
 
typedef struct s_Thing {
  string name;
} Thing;

typedef sequence <::Thing> ThingList;

module Mod0 {

  struct Thing {
    long lVal;
    short sVal;
    ::Thing thing;
  };
  
  interface Paul {
    Thing pop( in ThingList tl );
  };
  
  typedef sequence <Thing> ThingList;

  interface Peter {
    Thing pop( in ThingList tl );
  };

};
