<?xml version="1.0"?>

<project name="orb" default="all" basedir=".">

  <!-- ==================================================== -->
  <!--             Concurrency build file                   -->
  <!-- $Id: build.xml,v 1.2 2001-03-17 18:43:22 brose Exp $ -->
  <!-- ==================================================== -->

   <target name="init">
	<property name="dirs.base" value="../../.."/>
	<property name="classdir" value="${dirs.base}/classes"/>
	<property name="src" value="${basedir}/src"/>
   </target>

   <target name="all" depends="init">
	<javac srcdir="${src}"
               destdir="${classdir}"
	       includes="jacorb/concurrency/**/*.java"
	        />
   </target>

   <target name="clean">
   </target>

</project>

