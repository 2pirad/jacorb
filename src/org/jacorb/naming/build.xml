<?xml version="1.0"?>

<project name="security" default="all" basedir="../../../..">

  <!-- ==================================================== -->
  <!--            Naming build file                         -->
  <!-- $Id: build.xml,v 1.4 2002-03-12 11:32:46 jason.courage Exp $ -->
  <!-- ==================================================== -->


   <target name="init">
	<property name="dirs.base" value="${basedir}"/>
	<property name="classdir" value="${dirs.base}/classes"/>
	<property name="src" value="${dirs.base}/src"/>
   </target>

   <target name="all" depends="init,base"/>

   <target name="base">
	<javac debug="${debug}" srcdir="${src}"
               destdir="${classdir}"
	       includes="org/jacorb/naming/*.java, org/jacorb/naming/**/*.java"
        />
   </target>

   <target name="clean">
   </target>

</project>





